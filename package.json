{
    "name": "books_lib",
    "version": "1.0.0",
    "description": "",
    "isApp": false,
    "author": "Yuri Yaryshev",
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "exports": {
        ".": {
            "import": "./lib/esm/index.js",
            "require": "./lib/cjs/index.js"
        }
    },
    "typings": "lib/types/index.d.ts",
    "homepage": "https://github.com/yuyaryshev/books_lib",
    "repository": {
        "type": "git",
        "url": "https://github.com/yuyaryshev/books_lib"
    },
    "license": "Unlicense",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "precompile": "yb precompile",
        "precompile_full": "npm run precompile && prettier src --write",
        "clean:cjs": "yb clean_cjs",
        "build:cjs": "npm run clean:cjs && title books_lib - watch:cjs & babel src --config-file ./babel.cjs.config.cjs --out-dir lib/cjs --extensions \".ts,.tsx,.js,.jsx\" --source-maps",
        "watch:cjs": "npm run clean:cjs && title books_lib - watch:cjs & babel src --config-file ./babel.cjs.config.cjs --out-dir lib/cjs --extensions \".ts,.tsx,.js,.jsx\" --source-maps -w",
        "clean:esm": "yb clean_esm",
        "build:esm": "npm run clean:esm && babel src --config-file ./babel.esm.config.cjs --out-dir lib/esm --extensions \".ts,.tsx,.js,.jsx\" --source-maps && node mjs_import.test.mjs && echo mjs import is ok!",
        "watch:esm": "npm run clean:esm && babel src --config-file ./babel.esm.config.cjs --out-dir lib/esm --extensions \".ts,.tsx,.js,.jsx\" --source-maps -w",
        "clean:types": "yb clean_types",
        "build:types": "npm run clean:types && tsc -p tsconfig-declarations.json",
        "watch:types": "npm run clean:types && title books_lib - watch:types & tsc -p tsconfig-declarations.json -w",
        "test": "mocha './lib/cjs/**/*.test.js'"
    },
    "resolutions": {
        "yhttp_api": "^1.0.13",
        "yhttp_api_express": "^1.0.13",
        "@types/node": "^20.11.17"
    },
    "devDependencies": {
        "@babel/cli": "7.24.5",
        "@babel/core": "7.24.5",
        "@babel/node": "7.23.9",
        "@babel/parser": "7.24.5",
        "@babel/plugin-proposal-decorators": "7.24.1",
        "@babel/plugin-transform-modules-commonjs": "^7.24.1",
        "@babel/plugin-transform-typescript": "7.24.5",
        "@babel/preset-react": "^7.24.1",
        "@babel/preset-typescript": "^7.24.1",
        "@types/babel__core": "^7.20.5",
        "@types/chai": "4.3.16",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.12.12",
        "@types/react": "^18.3.2",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/ui": "^1.6.0",
        "babel-plugin-inline-replace-variables": "^1.3.1",
        "babel-plugin-module-resolver": "^5.0.2",
        "babel-watch": "^7.8.1",
        "cross-env": "^7.0.3",
        "javascript-stringify": "^2.1.0",
        "madge": "^7.0.0",
        "mocha": "^10.4.0",
        "modify-filepath": "*",
        "prettier": "^3.2.5",
        "pretty-quick": "^4.0.0",
        "source-map-support": "^0.5.21",
        "tslib": "^2.6.2",
        "typescript": "5.4.5",
        "vite": "^5.2.11",
        "yyabuilder": "^4.3.2"
    },
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@mui/icons-material": "^5.15.17",
        "@mui/material": "^5.15.17",
        "@types/better-sqlite3": "^7.6.10",
        "@types/express": "^4.17.21",
        "@types/luxon": "3.4.2",
        "axios": "^1.6.8",
        "better-sqlite3": "^10.0.0",
        "clone-deep": "^4.0.1",
        "express": "^4.19.2",
        "iconoir-react": "^7.7.0",
        "json5": "^2.2.3",
        "knex": "^3.1.0",
        "luxon": "3.4.4",
        "normalize.css": "^8.0.1",
        "ollama": "^0.5.16",
        "open-color": "^1.9.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.51.4",
        "react-resizable-panels": "^2.0.19",
        "react-router-dom": "^6.23.1",
        "yhttp_api": "^1.0.13",
        "yhttp_api_express": "^1.0.13",
        "ystd": "^3.13.0",
        "yuyaryshev-json-type-validation": "latest"
    }
}
